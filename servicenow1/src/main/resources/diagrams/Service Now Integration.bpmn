// find getUserNamesFromResponsibility
def getUserIdsFromResponsibility(ownerIds, resourceIds, roleIds) {
	def userIds = []
	def responsibilityFind = findResponsibilities(ownerIds, resourceIds, true, roleIds)
	loggerApi.info("responsibilityFind==>"+responsibilityFind)
	if(!responsibilityFind.isEmpty()){
		responsibilityFind.each {
			def eachOwner = it.getOwner()
			def eachOwnerId = eachOwner.getId()
			if(eachOwner.getResourceType() == ResourceType.UserGroup){
				def groupId = it.getOwner().getId()
				def groupUsers = findUsers(null,null,groupId,false)
				for(user in groupUsers) {
					userIds.add(user.getId())
				}
			}else {
				def ownerUserId = userApi.getUser(eachOwnerId).getId()
				userIds.add(ownerUserId)
			}
		}
	}
	return userIds;
}

