    <scriptTask id="scripttask7" name="Get updated service now attribute values of data usage" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script><![CDATA[import com.collibra.dgc.core.api.component.instance.AttributeApi
import com.collibra.dgc.core.api.dto.PagedResponse
import com.collibra.dgc.core.api.dto.instance.attribute.FindAttributesRequest
import com.collibra.dgc.core.model.attribute.Attribute

//Reusable code starts
def findAttributes(assetId, typeIds) {

	List<Attribute> attributes = new ArrayList<Attribute>();
	FindAttributesRequest.Builder builder = new FindAttributesRequest().builder();
	Boolean moreToProcess  = true;
	int offset = 0;
	while(moreToProcess){
		builder
				.offset(offset)
				.limit(1000)
		PagedResponse<Attribute> response  = attributeApi.findAttributes(builder.assetId(assetId).typeIds(typeIds).build());
		attributes.addAll(response.getResults());
		if(attributes.size()==response.total){
			moreToProcess = false;
		}
		else {
			offset = attributes.size();
		}
	}
	return attributes;
}//Reusable code ends

def serviceNowStatus
def serviceNowNumber

def dataUsageAssetName = assetApi.getAsset(item.id).getName()

//Get service now number and service now status attribute values of data usage for notifying consumer about service-now status
def finddataUsageAttr = findAttributes(item.id, [string2Uuid(serviceNowNumberAttrTypeId)])
if(!finddataUsageAttr.isEmpty()) {
	serviceNowNumber= finddataUsageAttr.get(0).getValue()
}

def serviceNowStatusAttr = findAttributes(item.id, [string2Uuid(serviceNowStatusAttrTypeId)])
if(!serviceNowStatusAttr.isEmpty()) {
	serviceNowStatus= serviceNowStatusAttr.get(0).getValue()
}

def dataUsageAssetURL = applicationApi.getInfo().getBaseUrl()+'asset/'+uuid2String(item.id)

//Email content and subject for notification
emailTextContent = "ServiceNow integration of <a href="+dataUsageAssetURL+">"+dataUsageAssetName+"</a> is successfully done with status - "+serviceNowStatus

emailTextSubjectLine = dataUsageAssetName+' || '+serviceNowNumber+' - '+serviceNowStatus

execution.setVariable("emailText",emailTextContent)
execution.setVariable("emailTextSubject",emailTextSubjectLine)
execution.setVariable("dataUsageAssetURL",dataUsageAssetURL)]]></script>
