    <scriptTask id="scripttask6" name="Triggering rest url for service now" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script><![CDATA[


import groovy.json.JsonOutput;
import groovy.json.JsonBuilder

loggerApi.info("Checkout || Service Now Integration Starts")

def dataUsageAssetId = item.id
def dataUsageId =item.id.toString()
loggerApi.info("dataUsageId==>"+dataUsageId)
def dataUsageAssetURL = applicationApi.getInfo().getBaseUrl()+'asset/'+uuid2String(item.id)

def urgencyRequestReasonval = urgencyRequestReason
loggerApi.info("urgencyRequestReasonval==>"+urgencyRequestReasonval.getClass())
loggerApi.info("urgencyRequestReason==>"+urgencyRequestReason.getClass())

def urgecnyAtr = attributeUpsert(dataUsageAssetId, urgencyRequestReason ,urgencyAttrTypeId)
def urgencyValues = urgecnyAtr.getValue()
loggerApi.info("urgecnyAtr==>"+urgecnyAtr)

def impactAtr = attributeUpsert(dataUsageAssetId, impactRequestReason, impactAttrTypeId)
loggerApi.info("impactAtr==>"+impactAtr)


def descriptionAtr = attributeUpsert(dataUsageAssetId,description ,descritionAttrtypeId)
loggerApi.info("descriptionAtr==>"+descriptionAtr)



def selectImpactSplit = impactRequestReason.split(" - ")[0]
def selectUrgencySplit = urgencyRequestReason.split(" - ")[0]
/******** Get DataUsage Asset Id ********/


def json = new JsonBuilder()
def root = json "dataUsageId": dataUsageId ,"description":description ,"dataElement":dataElement ,"callerName": serviceNowUserName ,"requesterNames" : requesterName ,"requesterMailID" : requesterMail, "dataUsageURL" : dataUsageAssetURL, "impactValue" : selectImpactSplit, "urgencyValue" : selectUrgencySplit
loggerApi.info("json.toString()==>"+json.toString())
def jsonOP = JsonOutput.toJson(root)

/******** Trigger Rest URL to Integrate serviceNow ********/
def post = new URL(restEndApiUrl+"/createIncident").openConnection();
loggerApi.info("post===>"+post)
def message = '{"message":"this is a message"}'
post.setRequestMethod("POST")
post.setDoOutput(true)
post.setRequestProperty("Content-Type", "application/json")
loggerApi.info("authorization ok==>")

def httpRequestBodyWriter = new BufferedWriter(new OutputStreamWriter(post.getOutputStream()))
loggerApi.info("httpRequestBodyWriter====>"+httpRequestBodyWriter)
httpRequestBodyWriter.write(jsonOP)
httpRequestBodyWriter.close()

/******** Check for Response Code ********/
def postRC = post.getResponseCode();
loggerApi.info("PostRc==>"+postRC)

if(postRC.equals(200)) {
requestBodyFromResponse = post.getInputStream().getText()
loggerApi.info("requestBodyFromResponse===>"+post.getInputStream())
def requestBodyFromResponseSplit= requestBodyFromResponse.split(",")
loggerApi.info("requestBodyFromResponseSplit====>"+requestBodyFromResponseSplit)
def dataUsageIdFromResponse= string2Uuid(requestBodyFromResponseSplit[0])
addAttribute(dataUsageIdFromResponse, string2Uuid(serviceNowNumberAttrTypeId), requestBodyFromResponseSplit[1])
addAttribute(dataUsageIdFromResponse, string2Uuid(serviceNowStatusAttrTypeId), requestBodyFromResponseSplit[2])
}
execution.setVariable("postRC",postRC)
execution.setVariable("requestBodyFromResponse",requestBodyFromResponse)
loggerApi.info("Rest Api Call Flow End")

execution.setVariable("postRC",postRC)
loggerApi.info("Rest Api Call Flow End")
loggerApi.info("Checkout || Service Now Integration Ends")


]]></script>
